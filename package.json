{
  "name": "dkcutter-turbo",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.15.1",
  "description": "DKCutter Turbo is a framework for quickly starting projects with Turborepo.",
  "author": {
    "name": "Nicolas Contiero",
    "url": "https://github.com/ncontiero"
  },
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "generate": "dkcutter .",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "check": "pnpm lint && pnpm typecheck",
    "commitlint": "commitlint --edit",
    "pre-commit": "lint-staged -c ./package.json",
    "prepare": "husky"
  },
  "dependencies": {
    "dkcutter": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@ncontiero/eslint-config": "^6.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.18.0",
    "colorette": "^2.0.20",
    "eslint": "^9.34.0",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "type-fest": "^4.41.0",
    "typescript": "^5.9.2",
    "zod": "^4.1.5"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*": "pnpm eslint --fix"
  }
}
