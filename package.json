{
  "name": "dkcutter-turbo",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.8.1",
  "description": "DKCutter Turbo is a framework for quickly starting projects with Turborepo.",
  "author": {
    "name": "Nicolas Contiero",
    "url": "https://github.com/ncontiero"
  },
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "generate": "dkcutter .",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "check": "pnpm lint && pnpm typecheck",
    "commitlint": "commitlint --edit",
    "pre-commit": "lint-staged -c ./package.json",
    "prepare": "husky"
  },
  "dependencies": {
    "dkcutter": "^4.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@ncontiero/eslint-config": "^5.1.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.14.1",
    "colorette": "^2.0.20",
    "eslint": "^9.24.0",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "type-fest": "^4.40.0",
    "typescript": "^5.8.3",
    "zod": "^3.24.3"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*": "pnpm eslint --fix"
  }
}
